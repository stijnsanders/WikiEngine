	with http://c2.com/cgi/wiki?$2|@@hspan%$1@@h/span%$2
//WikiPedia
replace ^(wikipedia:)(.+)$
	with http://en.wikipedia.org/wiki/$2|@@hspan%$1@@h/span%$2
//MeatBall
replace ^(usemod:)(.+)$
